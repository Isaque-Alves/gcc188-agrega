{
    "presets": [
        ["@babel/env", {
            "targets": {
                "node": "current"
            }
        }]
    ],
    "plugins": [
        ["@babel/transform-runtime"],
        ["babel-plugin-root-import", {
            "paths": [
                {
                    "rootPathSuffix": "./src",
                    "rootPathPrefix": "~/"
                }
            ]
        }]
    ]
}